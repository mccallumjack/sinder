<div class="container" id="repo-container">
  <div class="card large">
    
    <div id="repo-header" class="center-align">
      <h2 id="repo-title">Repo Name</h2>
      <h5 id="repo-description">Description</h5>
    </div>

    <div id="repo-attributes">
    <div class="container">
      <p>Language: <span id="repo-language"></span></p>
      <p>Stars: <span id="repo-stars"></span></p>
      <p>Forks: <span id="repo-forks"></span></p>
    </div>
    </div>

    <div class="center-align">
      <h5>Open Issues</h5>
        <table id="issue-list">
          
        </table>
    </div>

  </div>
</div>





<div id="display-buttons" class="center-align">
  <button id="hide">Hide</button>
  <button id="star">Like</button>
</div>